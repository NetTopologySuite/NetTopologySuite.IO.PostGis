	<files>
		<file src="NetTopologySuite.IO\NetTopologySuite.IO.PostGis\**" exclude="NetTopologySuite.IO\NetTopologySuite.IO.PostGis\obj\**" target="src\NetTopologySuite.IO\NetTopologySuite.IO.PostGis" />

		<file src="Release\AnyCPU\**\NetTopologySuite.IO.PostGis.deps.json" target="lib" />
